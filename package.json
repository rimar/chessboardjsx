{
  "name": "ggchessboard",
  "version": "1.0.2",
  "description": "Fork of Chessboard.jsx. A chessboard for React. Inspired by chessboard.js",
  "main": "dist/chessboard.min.js",
  "types": "chessboard.d.ts",
  "scripts": {
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "commit": "git-cz",
    "test": "jest --coverage",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint src",
    "validate": "npm-run-all --parallel test lint build",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "npx semantic-release",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "report-coverage": "cat ./coverage/lcov.info | codecov"
  },
  "keywords": [
    "react",
    "chess",
    "chessboard"
  ],
  "files": [
    "chessboard.d.ts",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rimar/chessboardjsx.git"
  },
  "author": "William .J Bashelor <wbashelor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rimar/chessboardjsx/issues"
  },
  "homepage": "https://github.com/rimar/chessboardjsx#readme",
  "dependencies": {
    "deep-diff": "1.0.2",
    "lodash.isequal": "4.5.0",
    "react-dnd": "10.0.2",
    "react-dnd-html5-backend": "10.0.2",
    "react-dnd-multi-backend": "6.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-object-rest-spread": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/register": "7.9.0",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/chess.js": "^0.10.1",
    "all-contributors-cli": "6.14.2",
    "babel-cli": "6.26.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "babel-polyfill": "6.26.0",
    "chess.js": "0.10.3",
    "clean-webpack-plugin": "3.0.0",
    "codecov": "3.6.5",
    "commitizen": "4.1.2",
    "css-loader": "3.5.3",
    "cz-conventional-changelog": "3.2.0",
    "eslint": "7.0.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "6.0.0",
    "ghooks": "2.0.4",
    "html-webpack-plugin": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.0.1",
    "jest-dom": "4.0.0",
    "kcd-scripts": "6.0.0",
    "lodash-webpack-plugin": "0.11.5",
    "mini-css-extract-plugin": "0.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "prop-types": "15.7.2",
    "react": "^16.4.0",
    "react-docgen": "5.3.0",
    "react-dom": "^16.4.0",
