    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run validate",
      "commit-msg": "npm run commitmsg"
    }
  }
}
